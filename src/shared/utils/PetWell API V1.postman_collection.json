				{
					"name": "Update Business Profile",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status code is 200', function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test('Response includes updated business profile', function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('id');",
									"    pm.expect(jsonData).to.have.property('business_name');",
									"    pm.expect(jsonData).to.have.property('email');",
									"    pm.expect(jsonData).to.have.property('profile_picture_document_id');",
									"    pm.expect(jsonData.profilePictureDocument).to.be.oneOf([null, pm.expect.any(Object)]);",
									"    if (jsonData.profilePictureDocument) {",
									"        pm.expect(jsonData.profilePictureDocument).to.have.property('id');",
									"        pm.expect(jsonData.profilePictureDocument).to.have.property('document_name');",
									"        pm.expect(jsonData.profilePictureDocument).to.have.property('file_type');",
									"    }",
									"    pm.expect(jsonData).to.have.property('socials').and.satisfy(val => val === null || typeof val === 'object');",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
