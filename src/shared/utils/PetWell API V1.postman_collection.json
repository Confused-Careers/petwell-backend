									"pm.test('Response includes paginated pet mappings', function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('data').and.to.be.an('array');",
									"    pm.expect(jsonData).to.have.property('total').and.to.be.a('number');",
									"    pm.expect(jsonData).to.have.property('page').and.to.equal('1');",
									"    pm.expect(jsonData).to.have.property('limit').and.to.equal('10');",
									"    if (jsonData.data.length > 0) {",
									"        pm.expect(jsonData.data[0]).to.have.property('map_id');",
									"        pm.expect(jsonData.data[0]).to.have.property('pet');",
									"        pm.expect(jsonData.data[0]).to.have.property('staff').and.satisfy(val => val === null || val.hasOwnProperty('id'));",
